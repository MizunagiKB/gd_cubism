ios.debug.universal = "bin/libgd_cubism.ios.debug.universal.dylib"
ios.release.universal = "bin/libgd_cubism.ios.release.universal.dylib"
android.debug.arm32 = "bin/libgd_cubism.android.debug.arm32.so"
