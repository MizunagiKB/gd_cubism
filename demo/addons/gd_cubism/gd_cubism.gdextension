[configuration]

entry_symbol = "gd_cubism_library_init"
compatibility_minimum = 4.1

[libraries]

macos.debug = "bin/libgd_cubism.macos.debug.framework"
macos.release = "bin/libgd_cubism.macos.release.framework"
windows.debug.x86_64 = "bin/libgd_cubism.windows.debug.x86_64.dll"
windows.release.x86_64 = "bin/libgd_cubism.windows.release.x86_64.dll"
linux.debug.x86_64 = "bin/libgd_cubism.linux.debug.x86_64.so"
linux.release.x86_64 = "bin/libgd_cubism.linux.release.x86_64.so"
# linux.debug.arm64 = "bin/libgd_cubism.linux.debug.arm64.so"
# linux.release.arm64 = "bin/libgd_cubism.linux.release.arm64.so"
# linux.debug.rv64 = "bin/libgd_cubism.linux.debug.rv64.so"
# linux.release.rv64 = "bin/libgd_cubism.linux.release.rv64.so"
ios.debug.arm64 = "bin/libgd_cubism.ios.debug.arm64.dylib"
